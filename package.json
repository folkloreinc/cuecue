{
    "name": "cuecue",
    "private": true,
    "scripts": {
        "babel-node": "./node_modules/.bin/babel-node --config-file=./babel.config.js  -i '[]'",
        "example:server": "DEBUG='cuecue:*' npm run babel-node -- ./examples/server.js",
        "example:lni": "DEBUG='cuecue:*' npm run babel-node -- --trace-warnings ./examples/lni.js ",
        "example:duration": "DEBUG='cuecue:*' npm run babel-node -- --trace-warnings ./examples/duration.js ",
        "example:treizelunes": "DEBUG='cuecue:*' npm run babel-node -- ./examples/treizelunes.js",
        "example:socketio": "DEBUG='cuecue:*' npm run babel-node -- ./examples/socketio.js",
        "debug:lni": "DEBUG='cuecue:*' nodemon --exec npm run babel-node -- ./examples/lni.js",
        "debug:treizelunes": "DEBUG='cuecue:*' nodemon --exec npm run babel-node -- ./examples/treizelunes.js",
        "migrate:store:up": "lerna exec --scope @cuecue/store-mysql -- npm run migrate:up",
        "migrate:store:down": "lerna exec --scope @cuecue/store-mysql -- npm run migrate:down"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/folkloreinc/cuecue.git"
    },
    "author": "Folklore <dev@folklore.email>",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/folkloreinc/cuecue/issues"
    },
    "homepage": "https://github.com/folkloreinc/cuecue#readme",
    "devDependencies": {
        "@babel/cli": "7.18.10",
        "@babel/core": "7.18.10",
        "@babel/eslint-parser": "7.18.9",
        "@babel/node": "7.18.10",
        "@babel/plugin-transform-runtime": "7.18.10",
        "@babel/preset-env": "7.18.10",
        "@babel/preset-react": "7.18.6",
        "@rollup/plugin-babel": "5.3.1",
        "@rollup/plugin-commonjs": "22.0.1",
        "@rollup/plugin-json": "4.1.0",
        "@rollup/plugin-node-resolve": "13.3.0",
        "dotenv": "^16.0.1",
        "eslint": "8.20.0",
        "eslint-config-airbnb": "19.0.4",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsx-a11y": "6.6.1",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.30.1",
        "nodemon": "^2.0.19",
        "prettier": "2.7.1",
        "rollup": "2.77.2",
        "stylelint": "14.9.1",
        "stylelint-config-idiomatic-order": "8.1.0",
        "stylelint-config-prettier": "9.0.3",
        "stylelint-config-standard-scss": "5.0.0"
    }
}
